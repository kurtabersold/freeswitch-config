<include>
    <gateway name="flowroute-outbound">
      <param name="username" value="{{ gateway.flowroute.auth_username }}"/>
      <param name="auth-username" value="{{ gateway.flowroute.auth_username }}"/>
      <param name="password" value="{{ gateway.flowroute.password }}"/>
      <param name="proxy" value="{{ gateway.flowroute.proxy }}"/>
      <param name="expire-seconds" value="600"/>
      <param name="register" value="true"/>
      <param name="retry-seconds" value="30"/>
      <param name="ping" value="25"/>
      <param name="dtmf-type" value="rfc2833"/>
    </gateway>
</include>
